
public class Cachorro {

	//atributos
	public String cor;
	public double peso;
	public String raca;
	public String acessorios;
	public double altura;
	
	//metodos
	public void latir() {
		System.out.println("au au");
	};

	public void escrever() {
		System.out.println("turma johnson baby");
	};
	public void morder(){
		System.out.println("vacilou, levou");
	}
	public void correr(){
		System.out.println("run run run");
	}
}
