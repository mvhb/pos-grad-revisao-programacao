public class Exemplos {

	public static void main(String[] args) {
		// Declaração de variáveis
		boolean a = false;
		
		int b, c;
		b= 2;
		c=3;
		
		double d = 2.2;
		float f = 3.2f;
		
		short s = 2;
		
		System.out.println("a:" + a);

	}

}
