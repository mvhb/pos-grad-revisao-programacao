
public class Exemplos {

	public static void main (String[] args) {
		
		//criando um objeto a partir da classe Cachorro
		
		Cachorro luppy = new Cachorro();
		
		luppy.setCor("black and white");
		luppy.getCor();
		
		
		// chamando os metodos
		luppy.correr();
		luppy.escrever();
		luppy.latir();
		luppy.morder();
	}
}
